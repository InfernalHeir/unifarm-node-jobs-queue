{
   "name": "unifarm-node-jobs-queue",
   "version": "1.0.0",
   "main": "./src/server.ts",
   "license": "MIT",
   "scripts": {
      "dev": "export NODE_ENV=dev && nodemon --ext '.ts' ./src/server.ts",
      "clean": "rimraf ./dist",
      "killport": "kill-port --port 8080",
      "build": "babel ./ --extensions '.ts' --out-dir ./dist --config-file ./.babelrc --no-copy-ignored --copy-files && rm -rf ./dist/node_modules ./dist/package.json ./dist/tsconfig.json",
      "server": "pm2 start -f ./dist/src/server.js",
      "start": "export NODE_ENV=prod && npm-run-all clean killport build server"
   },
   "devDependencies": {
      "@babel/core": "^7.14.2",
      "@babel/plugin-transform-modules-commonjs": "^7.14.0",
      "@babel/plugin-transform-runtime": "^7.14.2",
      "@babel/preset-env": "^7.14.2",
      "@babel/preset-typescript": "^7.13.0",
      "@babel/runtime": "^7.14.0",
      "@types/bull": "^3.15.1",
      "@types/express": "^4.17.11",
      "@types/node": "^15.0.2",
      "@typescript-eslint/eslint-plugin": "^4.22.1",
      "babel-plugin-transform-runtime": "^6.23.0",
      "eslint": "^7.25.0",
      "prettier": "^2.2.1",
      "ts-node": "^9.1.1",
      "typescript": "^4.2.4"
   },
   "dependencies": {
      "@babel/cli": "^7.13.16",
      "@ethersproject/contracts": "^5.1.1",
      "@ethersproject/keccak256": "^5.2.0",
      "@ethersproject/providers": "^5.1.2",
      "@ethersproject/units": "^5.1.0",
      "@ethersproject/wallet": "^5.1.0",
      "babel-plugin-dotenv-import": "^2.2.0",
      "babel-polyfill": "^6.26.0",
      "bluebird": "^3.7.2",
      "bull": "^3.22.4",
      "bull-board": "^2.0.1",
      "cors": "^2.8.5",
      "dotenv": "^9.0.1",
      "ethers": "^5.1.4",
      "fuse.js": "^6.4.6",
      "kill-port": "^1.6.1",
      "morgan": "^1.10.0",
      "node-fetch": "^2.6.1",
      "nodemailer": "^6.6.0",
      "npm-run-all": "^4.1.5",
      "pm2": "^4.5.6",
      "rimraf": "^3.0.2",
      "winston": "^3.3.3"
   }
}
